    "require": {
        "php": ">=8.0",
        "twig/twig": "^3.0",
        "phpmailer/phpmailer": "^6.8",
        "google/auth": "^1.28",
        "google/cloud-core": "^1.52",
        "kelvinmo/simplejwt": "^0.7.0",
        "vlucas/phpdotenv": "^5.5",
        "ext-pdo": "*",
