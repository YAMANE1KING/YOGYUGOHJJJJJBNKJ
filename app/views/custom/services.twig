{% include 'ht.twig' %}

<!-- Main variables *content* -->
		<style>
  .hidden,
  [hidden] {
    display: none;
  }
</style>
<div class="app-content">
  <div id="PageTitle">Services</div>
  <div class="container-fluid ">
    <section class="dashboard-head-wrapper">
      <div class="dashboard-head card-border">
        <img src="https://cdn.mypanel.link/dfdhtt/rlm42sazpgok8c5w.png" class="mascot-img"/>
        <div class="dhead-body">
          <h2 class="title">
            Our Services
          </h2>
          <p class="text">
            <a href="/tickets">Let us know</a>
            If you are an agency or API for reseller offers list.
          </p>
        </div>
      </div>
    </section>
    <section class="mb-4">
      <div class="card card-border">
        <div class="card-body">
          <div class="row align-items-center">
           <div class="col-md-4 col-12 col-lg-3">
                                            <a class="select-button" href="#" role="button" id="dropdown-category-filter-7" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <span class="fal fa-filter"> Filter </span>
                                                <span class="services-filter__active-category" data-filter-active-category="true"></span>
                                            </a>
                                            <div class="dropdown-menu"><a class="dropdown-item"  data-filter-category-id="All">All</a>{% for category in serviceCategory %}
                                                                                                                                                                                                <a class="dropdown-item" href="services#" data-filter-category-id="{{ category["category_id"] }}" data-filter-category-name="{{ category["category_name"] }}">{{ category["category_name"] }}</a>{% endfor %}
                                                                                                                                                                                                
                                                                                                                                          </div>
                                        </div>
            <!-- ./col -->
            <div class="col-md-4 ms-md-auto ms-md-0 col-12 mt-3 mt-md-0 col-lg-2">
              <div class="dropdown">
                <button class="select-button dropdown-toggle btn-block text-start" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                  <span class="selected-text">Currency</span>
                  <span class="select-arrow">
                    <svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24" width="16" height="16">
                      <path fill="none" d="M0 0h24v24H0z"></path>
                      <path d="M12 13.172l4.95-4.95 1.414 1.414L12 16 5.636 9.636 7.05 8.222z" fill="rgba(198,198,198,1)"></path>
                    </svg>
                  </span>
                </button>
                <ul class="dropdown-menu">   {% for curr in currencies %} <li class="nav-item component-navbar-nav-item component-navbar-private-nav-item"> <a class="dropdown-item" href="/account/currency_preferred/{{ curr["id"]}}"> {{ curr["symbol"]}} {{ curr["name"]}}</a></li>    {% endfor %}
       </ul>
              </div>
            </div>
            <!-- ./col -->
            <div class="col-lg">
              <div class="search-services">
                <div class="icon">
                  <i class="ri-search-line"></i>
                </div>
                <input type="text" class="textbox" name="query" id="filterServicesInput" placeholder="Search in services" data-search-service="#service-table">
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <div class="nothing-found" style="display: none;">
      <div class="card card-border">
        <div class="card-body p-4 p-lg-5 text-center">
          <div style="font-size: 64px">
            <i class="ri-search-eye-line w-color"></i>
          </div>
          <h3>Nothing found</h3>
          <p>We searched everywhere but couldn't find anything. Try with different keywords.</p>
          <div class="text-center d-none">
            <a href="/" class="btn btn-seconday">Create request for
              <span id="search-text-write"></span>
            </a>
          </div>
        </div>
      </div>
    </div>
    
    
    
    
    
    
    
